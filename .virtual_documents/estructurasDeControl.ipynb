palabra = "murcielago"
vocales = "aeiouAEIOU"

# 2. Set comprehension
# Se usan llaves {} en lugar de corchetes []
conjunto_vocales = {letra for letra in palabra if letra in vocales}

# 3. Resultado
print("Palabra original:", palabra)
print("Vocales encontradas (sin repetir):", conjunto_vocales)


palabra = "murcielago"

vocales = {letra for letra in palabra.lower() if letra in 'aeiou'}

print("Vocales únicas:", vocales)





# List comprehension: recorremos del 0 al 50
# Convertimos el número 'n' a string para usar el operador 'in'
lista_con_tres = [n for n in range(51) if '3' in str(n)]

# Mostramos el resultado
print(lista_con_tres)


numeros_con_3 = [n for n in range(51) if '3' in str(n)]

print("Números del 0 al 50 con dígito 3:", numeros_con_3)





# 1. Definimos la frase
frase = "Soy un ser humano"

# 2. Dictionary comprehension
# La clave es la 'palabra' y el valor es su longitud 'len(palabra)'
diccionario_tamanos = {palabra: len(palabra) for palabra in frase.split()}

# 3. Resultado
print(diccionario_tamanos)



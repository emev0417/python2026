# 1. Definimos una lista de palabras de prueba
lista_palabras = ["mesa", "computadora", "automovilístico", "silla", "albaricoque"]

# 2. Variable para saber si encontramos la palabra o no (un "interruptor")
encontrada = False
palabra_final = ""

# 3. Recorremos la lista con un for
for palabra in lista_palabras:
    # Comprobamos las dos condiciones: 
    # Que empiece por 'a' Y que su largo (len) sea mayor a 9
    if palabra.startswith('a') and len(palabra) > 9:
        encontrada = True
        palabra_final = palabra
        break  # Esto detiene el bucle inmediatamente

# 4. Mostramos el mensaje según el resultado
if encontrada:
    print("¡Éxito! Se ha encontrado la palabra:", palabra_final)
else:
    print("Fracaso: No existe ninguna palabra con esas condiciones.")


# 1. Definimos la lista de palabras
lista_palabras = ["mesa", "computadora", "automovilístico", "silla", "albaricoque", "astronomía"]

# 2. Variables de control
# Usamos una 'bandera' para saber si encontramos la palabra
encontrada = False
palabra_buscada = ""

# 3. Iniciamos el bucle for para revisar cada palabra
for palabra in lista_palabras:
    # Comprobamos las dos condiciones del enunciado
    # palabra[0] == 'a' mira si la primera letra es 'a'
    # len(palabra) > 9 cuenta si tiene más de 9 letras
    if palabra[0] == 'a' and len(palabra) > 9:
        encontrada = True
        palabra_buscada = palabra
        break  # Termina el programa en el momento exacto de encontrarla

# 4. Mensajes finales según el éxito o fracaso
if encontrada == True:
    print("Éxito: Se ha encontrado la palabra buscada.")
    print("La palabra es:", palabra_buscada)
else:
    print("Fracaso: No se encontró ninguna palabra que cumpla las condiciones.")



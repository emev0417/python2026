  {
   "cell_type": "code",
   "execution_count": 17,
   "id": "f69a4b6c-a206-4336-842b-703f2009ba3b",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "30xyz\n"
     ]
    }
